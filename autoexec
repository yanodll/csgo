bind "mouse4" "use weapon_knife; use weapon_c4; drop; say_team Dropping the bomb!" // drop the bomb and inform in chat
bind "kp_leftarrow" "echo faceit.dll has been injected" // write message in console
bind "z" "use weapon_knife;use weapon_flashbang; buy flashbang" // quick use and buy flashbang
bind "x" "use weapon_knife;use weapon_hegrenade; buy hegrenade" // quick use and buy hegrenade
bind "c" "use weapon_knife;use weapon_molotov;use weapon_incgrenade; buy molotov; buy incgrenade" // quick use and buy molotov or incgrenade
bind "v" "use weapon_knife;use weapon_smokegrenade; buy smokegrenade" // quick use and buy smokegrenade
